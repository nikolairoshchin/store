<h1>Warehouses#show</h1>
<div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @warehouse.name %></td>
        <td><%= @warehouse.address %></td>
        <td><%= link_to "Edit", edit_warehouse_path(@warehouse) %></td>
        <td><%= link_to "Destroy", @warehouse, data: { turbo_method: :delete, turbo_confirm: "Are you sure?" } %></td>
      </tr>
  </tbody>
</table>
</div>