<div class="container">
	<div class="row">
		<div class="col-md-4 offset-md-2">
			<%= form_for(@warehouse, data: { turbo: false }) do |f| %>
				<% @error_var = @warehouse %>
				<%= render 'shared/error_messages' %>

				<%= f.label :name %>
				<%= f.text_field :name, class: 'form-control' %>

				<%= f.label :address %>
				<%= f.text_field :address, class: 'form-control' %>

				<%= f.submit yield(:btn_title), class: "btn btn-primary" %>

			<% end %>
		
		</div>
	
	</div>
</div>